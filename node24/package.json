{
  "name": "node24-axios-test",
  "version": "1.0.0",
  "description": "Test url.parse() deprecation warning with Axios on Node.js 24",
  "main": "dist/test.js",
  "scripts": {
    "build": "tsc",
    "test": "tsc && node dist/test.js",
    "test:warnings": "tsc && node --trace-deprecation dist/test.js",
    "test:deprecation": "tsc && node --trace-deprecation dist/test-deprecation.js",
    "test:force": "tsc && node --trace-deprecation dist/test-force-deprecated.js",
    "test:lambda": "tsc && node --trace-deprecation dist/test-lambda.js",
    "test:throw": "tsc && node --throw-deprecation dist/test-deprecation.js",
    "dev": "ts-node src/test.ts",
    "dev:warnings": "node --trace-deprecation -r ts-node/register src/test.ts"
  },
  "dependencies": {
    "axios": "^1.7.0",
    "@aws-sdk/client-s3": "^3.700.0",
    "aws-sdk": "^2.1692.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.7.0"
  }
}
